	<script type="text/javascript">
	setTimeout(function () {
	    $(function () {
	        var height = $(window).innerHeight();
	        $(".doc_page").css({
	            minHeight: height
	        });

	        $(window).on("resize", function () {
	            var height = $(window).innerHeight();
	            $(".doc_page").css({
	                minHeight: height
	            });
	        });

	        if (window.location.hash != null) {
	            setTimeout(function () {

	                if (window.location.hash != null) {
	                    var hashObject = $(window.location.hash);

	                    if (hashObject.length != 0)
	                        $(document).scrollTop(hashObject.offset().top);
	                }
	            }, 400);
	        }

	        $("body").css({ backgroundColor: '#F7FAFB' });
	        setTimeout(function () {
	            $("section.doc_content").animate({ opacity: 1 }, 500);
	        }, 500);

	        $("#author_box, ul.menu").addClass("animate-left");

	        //$("#author_box, ul.menu").animate({ left: "-=320" }, 500);

	        $("#author_box").on("mouseenter", function () {
	            $("#author_box").removeClass("animate-left");
	            //$("#author_box").animate({ left: "+=320" }, 200);
	        });

	        $("#author_box").on("mouseleave", function () {
	            $("#author_box").addClass("animate-left");
	            //$("#author_box").animate({ left: "-=320" }, 200);
	        });

	        $('ul.menu a, .doc_page a').not('[target="_blank"]').click(function() {
	            var elementClicked = $(this).attr("href");
	            var destination = $(elementClicked).offset().top;
	            $("html:not(:animated),body:not(:animated)").animate({ scrollTop: destination}, 500 );

	            if (history != null)
	                history.replaceState(null, '', elementClicked);

	            return false;
	        });
	    });
	}, 0)
	</script>
